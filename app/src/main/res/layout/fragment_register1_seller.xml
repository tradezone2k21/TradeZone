<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".seller.register.Register1SellerFragment"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/phone_sell_register"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="104dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="104dp"
            android:backgroundTint="@android:color/black"
            android:ems="10"
            android:hint="@string/phone_no"
            android:inputType="phone"
            android:textColor="#000000"
            android:textColorHighlight="#000000"
            android:textColorHint="#494949"
            android:textColorLink="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <EditText
            android:id="@+id/name_sell_register"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="104dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="104dp"
            android:backgroundTint="@android:color/black"
            android:ems="10"
            android:hint="@string/name"
            android:inputType="textPersonName"
            android:text=""
            android:textColor="#000000"
            android:textColorHighlight="#000000"
            android:textColorHint="#494949"
            android:textColorLink="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/phone_sell_register" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="157dp"
            android:layout_height="165dp"
            android:layout_marginTop="78dp"
            android:src="@drawable/ic_buyer_graphic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>